{
  "name": "tilskudd-logs-stats",
  "description": "Statistics from the tilskudd logs",
  "version": "1.1.2",
  "license": "MIT",
  "private": true,
  "author": {
    "name": "Geir GÃ¥sodden",
    "email": "geir.gasodden@pythonia.no",
    "url": "https://github.com/zrrrzzt"
  },
  "main": "index.js",
  "engines": {
    "node": ">=8.6.0"
  },
  "scripts": {
    "test": "standard && nsp check && ava",
    "test-offline": "standard && ava",
    "coverage": "nyc ava",
    "coveralls": "nyc ava && nyc report --reporter=lcov && cat coverage/lcov.info | coveralls",
    "standard-fix": "standard --fix",
    "start": "micro",
    "dev": "micro-dev",
    "now-deploy": "npm test && now --npm && now alias"
  },
  "keywords": [
    "microservice",
    "micro",
    "tilskudd",
    "logs",
    "stats",
    "Telemark fylkeskommune"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/telemark/tilskudd-logs-stats.git"
  },
  "bugs": {
    "url": "https://github.com/telemark/tilskudd-logs-stats/issues"
  },
  "homepage": "https://github.com/telemark/tilskudd-logs-stats",
  "now": {
    "alias": [
      "stats.logs.tilskudd.mikrotjenester.win"
    ],
    "type": "npm",
    "dotenv": "production.env"
  },
  "devDependencies": {
    "ava": "0.22.0",
    "axios": "0.16.2",
    "coveralls": "2.13.3",
    "micro-dev": "1.2.3",
    "nsp": "2.8.1",
    "nyc": "11.2.1",
    "standard": "10.0.3",
    "test-listen": "1.0.2"
  },
  "dependencies": {
    "markdown-it": "8.4.0",
    "micro": "9.0.0",
    "mongojs": "2.4.1",
    "winston": "2.3.1",
    "winston-papertrail": "1.0.5"
  }
